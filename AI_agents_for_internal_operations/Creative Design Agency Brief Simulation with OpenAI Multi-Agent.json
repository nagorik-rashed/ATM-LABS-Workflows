      "parameters": {
        "jsCode": "// Input: items[0].json.output is a string containing JSON\n// Output: parsed and cleaned JSON fields\nreturn items.map(item => {\n  let data = {};\n  try {\n    // Parse the string inside \"output\"\n    data = JSON.parse(item.json.output);\n  } catch (e) {\n    throw new Error(\"Failed to parse AI output: \" + e.message);\n  }\n  return { json: data };\n});\n"
      },
